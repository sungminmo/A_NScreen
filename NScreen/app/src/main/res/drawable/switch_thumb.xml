<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true">
        <shape android:shape="rectangle" android:visible="true" android:dither="true" android:useLevel="false">
            <corners android:radius="@dimen/dimen_30"/>
            <size android:width="@dimen/dimen_30" android:height="@dimen/dimen_29" />
            <stroke android:width="@dimen/dimen_1" android:color="@color/switch_stroke_color" />
            <solid android:color="@color/switch_background_color" />
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle" android:visible="true" android:dither="true" android:useLevel="false">
            <corners android:radius="@dimen/dimen_30"/>
            <size android:width="@dimen/dimen_30" android:height="@dimen/dimen_29" />
            <stroke android:width="@dimen/dimen_1" android:color="@color/switch_stroke_color" />
            <solid android:color="@color/switch_background_color" />
        </shape>
    </item>
</selector>